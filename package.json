{
  "name": "da-da-a-game",
  "version": "1.0.0",
  "description": "打大A游戏 - 用TypeScript开发的经典纸牌游戏",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:prod": "NODE_ENV=production npm run build && NODE_ENV=production npx webpack",
    "build:dev": "npm run build && npx webpack",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "watch": "tsc -w",
    "serve": "python3 -m http.server 8000"
  },
  "keywords": ["游戏", "纸牌", "打大A", "TypeScript"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.0.0",
    "typescript": "^4.9.0",
    "ts-node": "^10.9.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0",
    "ts-loader": "^9.4.0"
  }
} 